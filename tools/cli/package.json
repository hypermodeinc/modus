{
  "name": "@modus/cli",
  "description": "The Modus CLI",
  "version": "0.0.0",
  "author": "Hypermode, Inc.",
  "bin": {
    "hyp": "bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.3.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/HypermodeAI/modus",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "modus",
    "helpClass": "./dist/custom/help",
    "dirname": "modus",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
      "sdk": {
        "description": "Modus SDK version manager"
      }
    },
    "plugins": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HypermodeAI/modus.git"
  },
  "bugs": {
    "url": "https://github.com/HypermodeAI/modus/issues"
  },
  "scripts": {
    "pack": "oclif pack tarballs --parallel --no-xz",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  }
}
