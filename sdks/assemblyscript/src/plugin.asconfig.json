{
  "options": {
    "lib": [
      "./assembly/overrides",
      "../../@assemblyscript/wasi-shim/assembly",
      "./node_modules/@assemblyscript/wasi-shim/assembly"
    ],
    "use": [
      "console=hypermode_console",
      "abort=hypermode_abort",
      "trace=hypermode_trace",
      "seed=wasi_seed",
      "crypto=wasi_crypto",
      "performance=wasi_performance",
      "process=wasi_process",
      "Date=wasi_Date"
    ],
    "exportStart": "_start",
    "exportRuntime": true
  },
  "targets": {
    "debug": {
      "debug": true
    },
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  }
}
