{
  "name": "@hypermode/modus-sdk-as",
  "version": "0.13.0-alpha1",
  "description": "Modus SDK for AssemblyScript",
  "author": "Hypermode Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "test": "ast run",
    "pretest": "ast build && tsc -p ./tests",
    "build:transform": "tsc -p ./transform",
    "prepare": "npm run build:transform",
    "lint": "eslint .",
    "pretty": "prettier --write .",
    "pretty:check": "prettier --check ."
  },
  "dependencies": {
    "@assemblyscript/wasi-shim": "^0.1.0",
    "json-as": "^0.9.21",
    "semver": "^7.6.3",
    "xid-ts": "^1.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.16.11",
    "as-test": "^0.3.5",
    "assemblyscript": "^0.27.30",
    "assemblyscript-prettier": "^3.0.1",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "visitor-as": "^0.11.4"
  },
  "overrides": {
    "assemblyscript": "$assemblyscript"
  },
  "exports": {
    "./transform": "./transform/lib/index.js",
    "./tools/assemblyscript-eslint": "./tools/assemblyscript-eslint.js"
  },
  "files": [
    "index.ts",
    "assembly/**/*.ts",
    "!assembly/**/__tests__",
    "models/**/*.ts",
    "!models/**/__tests__",
    "bin/build-plugin.js",
    "plugin.asconfig.json",
    "transform/lib/*.js",
    "tools/**"
  ]
}
