{
  "$schema": "https://schema.hypermode.com/modus.json",
  "endpoints": {
    "default": {
      "type": "graphql",
      "path": "/graphql",
      "auth": "bearer-token"
    }
  },
  "models": {
    // This defines a model from Hugging Face that is hosted on Hypermode.
    "my-classifier": {
      "sourceModel": "distilbert/distilbert-base-uncased",
      "connection": "hypermode",
      "provider": "hugging-face"
    }
  }
}
