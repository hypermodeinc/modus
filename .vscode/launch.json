{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Hypermode Runtime (local)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "--dgraph=http://localhost:8080",
        "--refresh=1s",
      ]
    },
   
  ],
  "inputs": [
    {
      "id": "s3bucket",
      "type": "promptString",
      "description": "Enter the S3 bucket name",
      "default": "sandbox-runtime-storage"
    },
    {
      "id": "s3folder",
      "type": "promptString",
      "description": "Enter the S3 storage folder name",
      "default": "shared"
    },
  ]
}
